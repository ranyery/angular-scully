{
  "name": "angular-scully",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "copy:ssg": "mv dist/static/* dist/ && rm -rf dist/angular-scully && rm -rf dist/static",
    "superbuild": "npm run build && npm run scully && npm run copy:ssg",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "scully": "rm -rf scully/**/*.js && npx scully --configFile=scully/scully.config.ts --noPrompt --removeStaticDist --logSeverity=error --handle404=index --serverTimeout=30000",
    "scully:serve": "npx scully serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.13",
    "@angular/cli": "~14.2.13",
    "@angular/compiler-cli": "^14.2.0",
    "@gammastream/scully-plugin-regex": "^2.0.3",
    "@notiz/scully-plugin-lazy-images": "^0.5.0",
    "@scullyio/init": "^2.1.41",
    "@scullyio/ng-lib": "^2.1.0",
    "@scullyio/scully": "^2.1.0",
    "@scullyio/scully-plugin-puppeteer": "^2.1.0",
    "@types/html-minifier-terser": "^5.1.1",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "scully-plugin-minify-html": "^6.0.0",
    "typescript": "~4.7.2"
  }
}
